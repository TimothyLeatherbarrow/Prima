<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tiggerbiggo.prima.view.sample.control.PreviewController">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ScrollPane pannable="true" HBox.hgrow="ALWAYS">
               <content>
                  <Pane fx:id="imagePane" prefHeight="200.0" prefWidth="200.0" />
               </content>
            </ScrollPane>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="200.0">
               <children>
                  <Label alignment="CENTER" maxHeight="30.0" maxWidth="1.7976931348623157E308" text="Preview Resolution:" VBox.vgrow="ALWAYS" />
                  <HBox fx:id="widthHeightBox" layoutX="10.0" layoutY="10.0" />
                  <HBox>
                     <children>
                        <Button fx:id="mulButton" alignment="CENTER" maxHeight="40.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="X2" HBox.hgrow="ALWAYS" />
                        <Button fx:id="divButton" alignment="CENTER" maxHeight="40.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="/2" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <Button fx:id="refreshButton" alignment="CENTER" maxHeight="40.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Refresh" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
